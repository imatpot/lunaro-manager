{
  "importMap": "import-map.json",

  "tasks": {
    "cache": "deno cache src/main.ts",
    "start": "deno run --allow-all --unstable src/main.ts",
    "start:watch": "denon run --allow-all --unstable src/main.ts"
  },

  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
