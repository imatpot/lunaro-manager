{
  "name": "lunaro-tracking-bot",
  "version": "2.0.0",
  "description": "Discord bot for tracking Lunaro players in a given server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc -w && tsc-alias -w",
    "start": "npm run build && node dist/index.js",
    "start:watch": "nodemon --legacy-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imatpot/lunaro-manager.git"
  },
  "author": "@imatpot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imatpot/lunaro-manager/issues"
  },
  "homepage": "https://github.com/imatpot/lunaro-manager#readme",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "date-fns": "^2.28.0",
    "discord-api-types": "^0.32.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "node-fetch": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
